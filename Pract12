package Pract12;

class Bank {

    private String name;

    Bank(String name) {

        this.name = name;
    }

    public String getBankName() {
        return this.name;
    }
}

class Employee {
    private String name;

    Employee(String name) {
        this.name = name;
    }

    public String getEmployeeName() {
        return this.name;
    }
}

public class Association {
    public static void main(String[] args) {
        Bank bank = new Bank("HDFC");
        Employee emp = new Employee("Jenis Patel");
        System.out.println(emp.getEmployeeName() + " is employee of " + bank.getBankName());
    }
}

Transition Class:

package Pract12;
class Bank1 {

    private String name;

    Bank1(String name) {

        this.name = name;
    }

    public String getBankName() {
        return this.name;
    }
}

class Employeee {
    private String name;

    Employeee(String name) {
        this.name = name;
    }

    public String getEmployeeName() {
        return this.name;
    }
}

class Branch{
    private String name;

    Branch(String name){
        this.name = name;
    }

    public String getBranceName() {
        return this.name;
    }
}
public class Transition
{
    public static void main(String[] args) {
        Bank1 bank = new Bank1("HDFC");
        Employeee emp = new Employeee("Jenis Patel");
        Branch brc = new Branch("Ahmedabad");
        System.out.println(emp.getEmployeeName() + " is employee of " + bank.getBankName() + " in " + brc.getBranceName() + " branch");
    }
}
